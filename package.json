    "ejs": "^2.6.1",
    "express": "~4.15.2",
    "express-session": "^1.15.6",
    "file-type": "^6.2.0",
    "jsonwebtoken": "^7.4.1",
    "moment": "^2.22.2",
    "morgan": "~1.8.1",
    "multer": "^1.3.0",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.0.1",
    "nodemailer-express-handlebars": "^2.0.0",
    "random-number": "0.0.9",
